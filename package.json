{
  "name": "ethiopian-payment-integration",
  "version": "1.0.0",
  "description": "Express MVC API with authentication and Ethiopian payment gateway integrations",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles"
  },
  "keywords": [
    "express",
    "mvc",
    "payments",
    "auth",
    "ethiopia"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.22.0",
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "dependencies": {
    "argon2": "^0.41.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "mysql2": "^3.11.0",
    "pino": "^9.4.0",
    "sequelize": "^6.37.7",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4"
  }
}
